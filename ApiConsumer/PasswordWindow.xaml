<Window x:Class="ApiConsumer.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiConsumer"
        mc:Ignorable="d"
        Title="PasswordWindow" Height="244" Width="800">
    <StackPanel>
        <Label Content="Username" Background="LightBlue" Margin="10" />
        <TextBox x:Name="tb_username" Margin="10" />
        <Label Content="Password" Background="LightBlue" Margin="10" />
        <PasswordBox x:Name="tb_pass" Margin="10" />
        <Button Content="LOGIN" Margin="10" Click="Button_Click"/>
    </StackPanel>
</Window>
